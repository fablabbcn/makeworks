<p id="notice"><%= notice %></p>

<h1>Character Tags</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @character_tags.each do |character_tag| %>
      <tr>
        <td><%= character_tag.name %></td>
        <td><%= link_to 'Show', character_tag %></td>
        <td><%= link_to 'Edit', edit_character_tag_path(character_tag) %></td>
        <td><%= link_to 'Destroy', character_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character Tag', new_character_tag_path %>
